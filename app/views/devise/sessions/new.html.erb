<div class="signin"> <div class="title"<h2>Sign in</h2></div></br>
<%= simple_form_for(resource, :as => resource_name, :url => session_path(resource_name), :html => {:class => 'form-vertical' }) do |f| %>
  <%= f.input :email, :autofocus => true %>
  <%= f.input :password %>
  <%= f.input :remember_me, :as => :boolean if devise_mapping.rememberable? %>
  <div id="signin"><%= f.button :submit, "Sign in", :class => 'btn-primary' %></div>
<% end %> 
<%= render "devise/shared/links" %></div>
<div id="homepage"></div>